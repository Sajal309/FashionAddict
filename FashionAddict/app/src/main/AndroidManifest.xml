<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.studyforfun.fashionaddict">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Men" />
        <activity android:name=".Women" />
        <activity android:name=".More" />
        <activity android:name=".Brand" />
        <activity android:name=".Account" />
        <activity android:name=".Cart" />
        <activity android:name=".Search" />
        <activity android:name=".choose" />
        <activity android:name=".Search2"></activity>
    </application>

</manifest>